<?xml version="1.0"?>

<mesh:mesh xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:mesh="XMLMesh"
  xmlns:element="XMLMesh"
  xsi:schemaLocation="XMLMesh ../../../../xml_schemas/mesh_h2d_xml.xsd">
  <vertices>
    <vertex x="0" y="0" i="0"/>
    <vertex x="2.0" y="0" i="1"/>
    <vertex x="2.0" y="1" i="2"/>
    <vertex x="0" y="1" i="3"/>
  </vertices>

  <elements>
    <element:quad v1="2" v2="3" v3="0" v4="1" marker="Mat" />
  </elements>

  <edges>
    <edge v1="2" v2="3" marker="Bdy" />
    <edge v1="3" v2="0" marker="Bdy" />
    <edge v1="0" v2="1" marker="Bdy" />
    <edge v1="1" v2="2" marker="Bdy" />
  </edges>

  <curves>
    <NURBS v1="2" v2="3" degree="3">
      <inner_point x="1.5" y="1.5" weight="1.0" />.
      <inner_point x="0.5" y="0.5" weight="1.0" />
      <knot value="0" />
      <knot value="0" />
      <knot value="0" />
      <knot value="1" />
      <knot value="1" />
      <knot value="1" />
    </NURBS>
  </curves>
</mesh:mesh>